{
  "name": "fate-sheet",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://ellerycrane.github.io/fate-sheet",
  "devDependencies": {
    "gh-pages": "^0.12.0",
    "node-sass": "^3.13.0",
    "react-scripts": "0.7.0"
  },
  "dependencies": {
    "react": "^15.4.0",
    "react-dom": "^15.4.0"
  },
  "scripts": {
    "start": "npm run watch-css & react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "build-css": "node-sass src/sass/base.scss src/compiled-css/index.css",
    "watch-css": "node-sass src/sass/base.scss src/compiled-css/index.css -w",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build",
    "build-deploy": "npm run build && gh-pages -d build"
  }
}
